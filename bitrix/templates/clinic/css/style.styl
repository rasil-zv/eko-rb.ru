.header_top
    text-align right
    padding 10px 0px
    display flex
    justify-content space-between
    align-items center
    +below($sm)
        display none
    .mobile_par
        +below($sm)
            display flex
            align-items center
    .left
        .btn
            margin 0px 15px 0px 0px
            &:last-of-type
                margin-right 0px
            +below($sm)
                margin 0px
                margin-bottom 10px
    a
        display inline-flex
        align-items center
        margin-left 25px
        font-size 14px
        transition all 0.1s ease-in-out
        +below($sm)
            margin 0px
            font-size 12px
        &:hover
            i
                color $color
        i
            padding-right 10px
            transition all 0.1s ease-in-out
            +below($sm)
                font-size 14px
        &.user
            +below($sm)
                p
                    display none
        &.contacts
            +below($sm)
                i
                    padding-right 0px
    .left
    .right
        text-align center
.header
    align-items center
    padding 25px 0px
    +below($sm)
        padding 15px 0px
    .block
        padding 0px 30px
        +below($sm)
            padding 0px 0px 10px 0px
            &:last-of-type
                padding-bottom 0px
        &:first-of-type
            padding-left 0px
            +below(991px)
                padding-right 0px
        &:last-of-type
            padding-right 0px
        &.logo
            img
                max-width 100%
                max-height 70px
        &.search
            padding-top 15px
            padding-left 0px
            form
                display flex
                align-items center
                input[type="text"]
                    border 1px solid $color
                    width 100%
                    height 35px
                    padding 0px 10px
                    &:focus
                        border 1px solid $color_hover
                a
                    height 35px
                    background-color $color
                    color white
                    display flex
                    align-items center
                    padding 0px 15px
                    font-size 14px
                    transition all 0.1s ease-in-out
                    &:hover
                        background-color $color_hover
        &.contacts
            +below($sm)
                display none
            ul
                li
                    display flex
                    align-items center
                    font-size 14px
                    +below($sn)
                        font-size 12px
                    i
                        padding-right 10px
                        color $color
                        font-size 14px
        &.phone
            text-align center
            .phone_number
                a
                    font-weight bold
                    transition all 0.1s ease-in-out
                    color $color
                    font-size 22px
                    +below($sm)
                        font-size 18px
                    &:hover
                        color $color_hover
            .feedback
                display inline-block
                font-size 16px
                text-decoration underline
                transition all 0.1s ease-in-out
                color #555
                margin-top 10px
                +below($sm)
                    font-size 12px
                &:hover
                    color #333
+above($sm)
    .nav_button
        display none
    .main_nav
        >ul
            display flex
            align-items center
            justify-content center
            +below($sm)
                display block
            >li
                position relative
                .arrow
                    display none
                >p
                    cursor default
                >a
                >p
                    display inline-block
                    padding 10px 25px
                    transition all 0.1s ease-in-out
                    font-size 14px
                    +below($sm)
                        display block
                        padding 10px 15px
                    &.active
                        background-color $color_hover
                        &:hover
                            opacity 0.8
                    &:hover
                        background-color $color_hover
                &:hover
                    >div>ul
                    >ul
                        opacity 1
                        visibility visible
                        transform translateX(0) !important
                &:last-of-type
                &:nth-last-child(2)
                    >div>ul
                    >ul
                        left auto
                        right 0
                        transform translateX(-20px)
                >div>ul
                >ul
                    box-shadow 0px 0px 15px -3px RGGBA(0,0,0,0.5)
                    position absolute
                    top 100%
                    left 0
                    z-index 50
                    background-color #fafafa
                    border 1px solid #999
                    transition all 0.2s ease-in-out
                    transform translateX(20px)
                    opacity 0
                    visibility hidden
                    min-width 200px
                    >li
                        >a
                            display block
                            transition all 0.1s ease-in-out
                            color #333
                            padding 7px 15px
                            font-size 14px
                            white-space nowrap
                            &:hover
                                background-color #eee
                            &.active
                                background-color $color
                                color white
                                &:hover
                                    opacity 0.8
                                    background-color $color
.banner
    height 350px
    overflow hidden
    position relative
    +below($sm)
        height 250px
    .style_dots
        position absolute
        bottom 25px
        right 25px
        +below(992px)
            display none
    .item
        height 350px
        background-size cover
        background-position center
        display flex
        align-items center
        position relative
        &:before
            content ''
            position absolute
            top 0
            left 0
            right 30%
            bottom 0
            background linear-gradient(90deg, RGBA(255,255,255,0.7) 80%, RGBA(255,255,255,0))
        +below($sm)
            height 250px
            padding 25px 0px
        .title
            font-size 26px
            margin-bottom 10px
            position relative
            +below(991px)
                font-size 18px
        .desc
            font-size 16px
            position relative
            font-style italic
            margin-bottom 25px
            max-width 600px
        .button
            position relative
.slick_nav
    visibility hidden
    opacity 0
    transition all 0.2s ease-in-out
    button
        position absolute
        height 100%
        width 10%
        &.left
            left 0
        &.right
            right 0
.btn
    padding 10px 20px
    text-transform uppercase
    display inline-block
    font-weight bold
    transition all 0.2s ease-in-out
    box-shadow 1px 1px 5px RGBA(0,0,0,0.5)
    cursor pointer
    font-family $font
    font-size 14px
    margin-right 25px
    text-align center
    &:last-of-type
        margin-right 0px
    &:hover
        box-shadow 3px 3px 8px RGBA(0,0,0,0.5)
        transform translateY(-1px)
    &.white
        background-color white
        color #616161
    &.purple
        background-color $color
        color white
    &.big
        font-size 16px
    &.green
        background-color #009734
        color white
    &.grey
        background-color #9b9b9b
        color white
    &.lightgrey
        background-color #eee
        color #333
    &.small
        font-size 13px
        font-weight 300
        box-shadow none
    &.no_shadow
        box-shadow none !important
    &.transparent
        background-color transparent
        border 2px solid white
        color white
        &:hover
            background-color white
            color $color
    &.no_animate
        transform none !important
        &:hover
            opacity 0.8
